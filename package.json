{
  "name": "farmhelden-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all -p start:css start:js",
    "start:js": "craco start",
    "start:css": "yarn run generate:css -w",
    "prebuild": "yarn run generate:css --env production",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "build:staging": "cp ./.env.staging ./.env && yarn run build",
    "build:production": "cp ./.env.prod ./.env && yarn run build",
    "generate:css": "postcss src/styles/main.src.css -c tailwind.config.js -o src/styles/main.css"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-actions": "^2.6.1",
    "@types/redux-immutable": "^4.0.1",
    "@types/webpack-env": "^1.15.1",
    "@types/yup": "^0.26.34",
    "autoprefixer": "^9.7.5",
    "husky": "^4.2.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "redux-devtools-extension": "^2.13.8",
    "tailwindcss": "^1.2.0",
    "tailwindcss-filters": "^3.0.0"
  },
  "dependencies": {
    "@craco/craco": "^5.6.4",
    "@material-ui/core": "^4.9.8",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@turf/bbox": "^6.0.1",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "esri-leaflet": "^2.3.3",
    "esri-leaflet-geocoder": "^2.3.2",
    "formik": "^2.1.4",
    "history": "^4.10.1",
    "immutable": "^4.0.0-rc.12",
    "leaflet": "^1.6.0",
    "leaflet.locatecontrol": "0.71.1",
    "leaflet.markercluster": "^1.4.1",
    "mapbox-gl": "^1.9.0",
    "mapbox-gl-leaflet": "^0.0.12",
    "moment": "^2.24.0",
    "react": "^16.13.1",
    "react-calendar": "^3.0.1",
    "react-dom": "^16.13.1",
    "react-feather": "^2.0.3",
    "react-leaflet": "^2.6.3",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-table": "^7.0.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-auth-wrapper": "^3.0.0",
    "redux-immutable": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "typescript": "~3.7.2",
    "use-mobile-detect-hook": "^1.0.3",
    "yup": "^0.28.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
